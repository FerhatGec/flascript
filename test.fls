import(<stdlib/output/Colorized.flsh>) -> colors <-
import(<stdlib/info/SystemInfo.flsh>) -> sysinfo <-

func -> Test() {
        print(newline)
        put[colors -> BOLD_RED <-]
        print(string) -> "Hello, world!"
        put[colors -> BLACK_COLOR <-]
} Test;

() -> main {
        print(string) -> "Terminal: "
        put[sysinfo -> func -> GetTerminal() <-]
	func -> Test()
} main;